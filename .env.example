# Environment variables for Docker Compose
# Copy this file to .env and set your actual values
GITLAB_MCP_URL="http://127.0.0.1:3333/mcp"

# GitLab MCP Configuration
GITLAB_PERSONAL_ACCESS_TOKEN=glpat-your_gitlab_personal_access_token_here
GITLAB_API_URL=https://gitlab.com/api/v4
GITLAB_READ_ONLY_MODE=true
USE_GITLAB_WIKI=true
USE_MILESTONE=true
USE_PIPELINE=true
STREAMABLE_HTTP=true

# Optional: Set specific versions
DEVOPS_MCP_VERSION=latest
GITLAB_MCP_VERSION=latest
